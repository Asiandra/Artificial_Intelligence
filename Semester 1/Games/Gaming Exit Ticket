    "def scatter(df, x, y, outliers):\n",
    "    idx=[]\n",
    "    for i in range(outliers):\n",
    "        idx=df[x].idxmax()\n",
    "        df.drop([idx], inplace=True)\n",
    "    sns.scatterplot(data=df, x=x, y=y)\n",
    "scatter(df=df, x='price_final', y='price_original', outliers=50)"
   ]
